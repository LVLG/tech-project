{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"sourceMap": true,
		"outDir": "dist",
		"rootDir": "src",
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
		"lib": ["ES2023"],
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["src"]
} 